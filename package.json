{
  "name": "QHIN",
  "version": "0.0.4",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "lint-old": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate ./migrations/newest --dataSource ./typeOrm.config.ts",
    "migration:revert": "npm run typeorm:cli -- migration:revert --dataSource ./typeOrm.config.ts",
    "migration:run": "npm run typeorm:cli -- migration:run  --dataSource ./typeOrm.config.ts",
    "prebuild": "rimraf dist",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "schema:sync": "npm run typeorm:cli -- schema:sync  --dataSource ./typeOrm.config.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typeorm:cli": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./typeOrm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.5",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/express": "^4.17.8",
    "@types/jwk-to-pem": "^2.0.1",
    "ajv": "^8.10.0",
    "ajv-keywords": "^5.1.0",
    "ansi-regex": ">=6.0.1",
    "auth0": "^4.0.1",
    "axios": "^1.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.0",
    "jwk-to-pem": "^2.0.5",
    "jwks-rsa": "^3.0.0",
    "mysql": "^2.18.1",
    "nest": "^0.1.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.1",
    "rxjs": "^7.5.2",
    "sane": "^5.0.1",
    "swagger-ui-express": "^5.0.0",
    "ts-loader": ">=9.2.6",
    "ts-node": ">=10.4.0",
    "tsconfig-paths": "^4.0.0",
    "typeorm": "^0.3.0",
    "typescript": "^5.1.3",
    "webpack": "^5.74.0",
    "yarn": "^1.22.17"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.7",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/testing": "^10.0.5",
    "@types/auth0": "^3.3.2",
    "@types/jest": "^29.0.3",
    "@types/node": "^20.2.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "onchange": "^7.1.0",
    "prettier": "^3.0.0",
    "supertest": "^7.0.0"
  }
}
